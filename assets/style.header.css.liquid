    position: relative;
    line-height: 1;
    display: flex;
    justify-content: center;
    align-items: center;
}

.header .header-control .header-control-icon{
    font-size: 23px;
}

.header .header-icon .header__counter {
    position: absolute;
    bottom: -6px;
    left: 10px;
    color: var(--color-button);
    font-weight: 600;
    width: 18px;
    height: 18px;
