/* Hero Canvas */
.hero_canvas{
    position: fixed;
    top: 0;
    bottom: 0;
    width: 100%;
    height: 100vh;
    max-width: 440px;
    background-color: #fff;
    right: 0;
    overflow-y: auto;
    overflow-x: hidden;
    -webkit-overflow-scrolling: touch;
    transition: all 0.3s ease-in-out;
    -webkit-transform: translate3d(104%,0,0);
    transform: translate3d(104%,0,0);
    opacity: 0;
    visibility: hidden;
    z-index: 99999;
    scrollbar-width: thin;
}

.hero_canvas::-webkit-scrollbar {
    width: 0;
}

.hero_canvas.act_opened{
    -webkit-transform: none;
    transform: none;
    opacity: 1;
    visibility: visible;
}

.mask-overlay{
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100vh;
    z-index: 9995;
    opacity: 0;
    visibility: hidden;
    pointer-events: auto;
    background-color: rgba(0,0,0,.7);
    transition: opacity .25s,visibility .25s;
}

.mask_opened{
    pointer-events: auto;
    opacity: 1;
    visibility: visible;
    transition: opacity .25s,visibility;
}

.icon-close{
    display: inline-block;
    width: 20px;
    height: 20px;
    position: absolute;
    right: 20px;
    top: 50%;
    transform: translateY(-50%);
    cursor: pointer;
}

.icon-close:before,
.icon-close:after{
    content: "\20";
    position: absolute;
    top: 50%;
    display: inline-block;
    margin-top: -1px;
    width: 20px;
    height: 2px;
    background-color: var(--color-main);
    transition: background-color .2s ease-in-out,transform .2s ease-in-out,width .2s ease-in-out;
    right: 0;
    transform: rotate(-45deg);  
}

.icon-close:before{
    transform: rotate(45deg);
}

.icon-close:hover:before,
.icon-close:hover:after{
    transform: rotate(0deg);
}
/* End Hero Canvas */

