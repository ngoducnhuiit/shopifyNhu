/* End Header Tool  */

/* Header Sticky */
.header-fixed.sticky{
    box-shadow: 0 3px 10px #0000001a;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    z-index: 11;
    transition: all 0.3s ease;
}

.header-fixed.sticky .header-sticky-none{
    display: none;
}

.scrollDown .header-fixed.sticky{
    transform: translateY(-100%);
}

/* menu label */
