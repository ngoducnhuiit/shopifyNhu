    opacity: 1;
}

.main-menu .menu-item.type_dropdown{
    position: relative;
}

.main-menu .menu-item.type_dropdown.has-children >.submenu{
    top: 100%;
    left: 0;
    transition: all .3s ease;
    transform: translateY(20px);
    border-radius: 0 0 8px 8px;
}

.main-menu .menu-item.type_dropdown.has-children:hover >.submenu{
    transform: translateY(0);
}

.main-menu .menu-item.type_dropdown .menu-item-has-children{
    position: relative;
}

.main-menu .menu-item.type_dropdown .menu-item-has-children >.menu-item-link:after{
    content:"\e063";
    font-family: 'organey-icon';
    display: inline-block;
    position: absolute;
    top: 50%;
    transform:translateY(-50%);
    right: -10px;
}

.main-menu .menu-item.type_dropdown .menu-item-has-children:hover >.menu-item-link{
    padding-left: 15px;
}

.main-menu .menu-item.type_dropdown .menu-item-has-children:hover >.menu-item-link:before{
    opacity: 1;
}

.main-menu .menu-item.type_dropdown .menu-item-has-children .submenu{
    opacity: 0;
    visibility: hidden;
    left: 100%;
    top:30px;
    border-radius: 8px;
}

.main-menu .menu-item.type_dropdown .menu-item-has-children:hover >.menu-item-link{
    color: var(--color-hover);
}

.main-menu .menu-item.type_dropdown .menu-item-has-children:hover >.menu-item-link:before{
    opacity: 1;
}

.main-menu .menu-item.type_dropdown .menu-item-has-children:hover >.submenu{
    opacity: 1;
    visibility: visible;
    top: 0;
}

.main-menu .menu-item.type_mega.has-children .submenu.mega-menu{
    left: 50%;
    transform: translate(-50%, 20px);
    transition: all .3s ease;
}

.main-menu .menu-item.type_mega.has-children:hover .submenu.mega-menu{
    transform: translate(-50%, 0);
}

.main-menu .menu-item.type_mega.has-children .submenu.mega-menu ul{
    padding: 0;
}

.main-menu .menu-item.type_mega.has-children .submenu.mega-menu .heading{
    font-size: 18px;
    font-weight: 600;
    margin-bottom: 10px;
    display: block;
    padding: 0 20px;
} 

.main-menu .menu-item.type_mega.has-children .submenu.mega-menu .heading:before{
    width: 0;
    height: 0;
}

.main-menu .menu-item .submenu .product-inner a:before{
