@charset "UTF-8";

/* Start Top Bar */
.top-bar-links__content .top-contact,
.top-bar-links__content .menu-links{
    display: inline-block;
    vertical-align: middle;
}

.top-bar-links__content .top-contact ul li{
    display: inline-block;
    vertical-align: middle;
    margin-right: 30px;
}

.top-bar-links__content ul li:last-child{
    margin-right: 0;
}

.top-bar-links__content .menu-language {
    margin-left: 30px;
}

.menu-links-fixed{
    position: fixed;
    right: 25px;
    bottom: 25px;
    z-index: 10;
    transition: all .3s ease;
    cursor: pointer;
}

.show-back-to-top .menu-links-fixed{
    bottom: 90px;
}

.show-sticky.show-back-to-top .menu-links-fixed{
    bottom: 155px;
}

.show-sticky.show-back-to-top .signin-fixed{
    bottom: 220px;
}

.scroll-bottom-page .menu-links-fixed{
    bottom: 25px;
}

.menu-links-fixed .toggle-tab{
    width: 45px;
    height: 45px;
    line-height: 45px;
    border-radius: 50%;
    text-align: center;
    transition: all .3s ease;
    color: var(--color-button);
    background-color: var(--background-button);
    position: relative;
    z-index: 10;
    -webkit-animation: rotate-center 7s linear infinite both;
    animation: rotate-center 7s linear infinite both;
}

.menu-links-fixed .toggle-tab:before{
    content: "\f0ac";
    font-family: Fontawesome;
    font-size: 20px;
}

.menu-links-fixed .toggle-tab:after{
    content: "";
    border-width: 1px;
    border-style: dashed;
    border-color: var(--border-button);
    position: absolute;
    width: calc(100% + 4px);
    height: calc(100% + 4px);
    border-radius: 100%;
    top: -3px;
    left: -3px;
}

.menu-links-fixed .parent-list{
    position: absolute;
    bottom: 0;
    right: 0;
    transition: all .3s ease;
    background-color: #fff;
    box-shadow: 0 0 2px #0003;
    opacity: 0;
    visibility: hidden;
    padding: 10px;
    display: block;
    z-index: 8;
    width: 200px;
    pointer-events: none;
}

.menu-links-fixed.toggle-visible .parent-list{
    right: calc(100% + 10px);
    opacity: 1;
    visibility: visible;
    pointer-events: unset;
}

.menu-links-fixed .parent-list .item-list{
    margin-right: 0;
    display: block;
    margin-bottom: 20px;
}

.menu-links-fixed .parent-list .item-list:last-child{
    margin-bottom: 0;
}

.menu-links-fixed .parent-list .item-list .child-list{
    position: static;
    padding: 0;
    border-width: 0;
    opacity: 1;
    visibility: visible;
}

.menu-links-fixed .parent-list .item-list:before {
    content: attr(data-title);
    margin-bottom: 10px;
    display: block;
    text-align: initial;
    font-weight: 500;
    color: var(--color-main);
}

.menu-links-fixed .parent-list .child-list li{
    display: inline-block;
    margin-right: 7px;
    margin-bottom: 7px;
}

.menu-links-fixed .parent-list .child-list li a{
    border-width: 1px;
    border-style: solid;
    border-color: var(--color-border);
    border-radius: 30px;
    padding: 0 15px;
    height: 30px;
    line-height: 28px;
    display: block;
}

.menu-links-fixed .parent-list .child-list li.active a, 
.menu-links-fixed .parent-list .child-list a:hover{
    background-color: var(--background-button);
    color: var(--color-button);
    border-color: var(--border-button);
    
} 

.signin-fixed{
    position: fixed;
    right: 25px;
    bottom: 90px;
    z-index: 10;
    transition: all .3s ease;
    cursor: pointer;
}

.show-back-to-top .signin-fixed{
    bottom: 155px;
}

.scroll-bottom-page .signin-fixed{
    bottom: 90px;
}

.signin-fixed .header-icon{
    width: 45px;
    height: 45px;
    line-height: 45px;
    border-radius: 50%;
    text-align: center;
    transition: all .3s ease;
    color: var(--color-button);
    background-color: var(--background-button);
    position: relative;
    z-index: 10;
    display: block;
}

.signin-fixed .header-icon:after{
    content: "";
    border-width: 1px;
    border-style: dashed;
    border-color: var(--border-button);
    position: absolute;
    width: calc(100% + 4px);
    height: calc(100% + 4px);
    border-radius: 100%;
    top: -3px;
    left: -3px;
}
/* End Top Bar */

/* Header Tool  */
