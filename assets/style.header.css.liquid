        display: inline-block;
    }

    .header-nav-mobile .menu-bar .icon{
        position: relative;
        width: 24px;
        height: 17px;
        display: block;
    }

    .header-nav-mobile .menu-bar .icon span{
        position: absolute;
        left: 0;
        height: 2px;
        background-color: var(--color-main);
        display: block;
        transition: all .3s ease;
    }

    .header-nav-mobile .menu-bar .icon span:first-child{
        top: 0;
        width: 80%;
    }

    .header-nav-mobile .menu-bar .icon span:nth-child(2){
        top: 50%;
        transform: translateY(-50%);
        width: 50%
    }

    .header-nav-mobile .menu-bar .icon span:last-child{
        bottom: 0;
        width: 100%
    }

    .header-nav-mobile .menu-bar .icon:hover span:first-child{
        top: 0;
        width: 50%;
    }

    .header-nav-mobile .menu-bar .icon:hover span:nth-child(2){
        top: 50%;
        transform: translateY(-50%);
        width: 100%
    }

    .header-nav-mobile .menu-bar .icon:hover span:last-child{
        bottom: 0;
        width: 80%
    }
}

@media(max-width: 991px){
    .header-search{
        display: none;
    }

    .show-sticky.show-back-to-top .menu-links-fixed{
        bottom: 90px;
    }

    .show-sticky.show-back-to-top .signin-fixed{
        bottom: 155px;
    }
}

@media(max-width: 767px){
    .cms-top-bar .topbar .top-bar-links{
        display: none;
    }

    .announcement-bar .wrapper-bar-message{
        text-align: center;
    }

    .menu-links{
        position: fixed;
        right: 25px;
        bottom: 25px;
        z-index: 9;
        transition: all .3s ease;
    }

    .menu-links .toggle-tab{
        width: 45px;
        height: 45px;
        line-height: 45px;
        border-radius: 50%;
        text-align: center;
        transition: all .3s ease;
        color: var(--color-button);
        background-color: var(--background-button);
        position: relative;
        z-index: 9;
        -webkit-animation: rotate-center 7s linear infinite both;
        animation: rotate-center 7s linear infinite both;
    }

    .menu-links .toggle-tab:before{
        content:"\f0ac";
        font-family: Fontawesome;
        font-size: 20px;
    }

    .menu-links .toggle-tab:after{
        content: "";
        border-width: 1px;
        border-style: dashed;
        border-color: var(--border-button);
        position: absolute;
        width: calc(100% + 4px);
        height: calc(100% + 4px);
        border-radius: 100%;
        top: -3px;
        left: -3px;
    }

    .show-back-to-top .menu-links{
        bottom: 0px;
    }

    .scroll-bottom-page .menu-links{
        transform: translate(100px);
        transition: all .3s ease;
    }

    .show-back-to-top .menu-links{
        bottom: 90px;
    }

    .menu-links .parent-list{
        position: absolute;
        bottom: 0;
        right: 0;
        transition: all .3s ease;
        background-color: white;
        box-shadow: 0 0 2px rgba(0, 0, 0, .2);
        opacity: 0;
        visibility: hidden;
        padding: 10px;
        display: block;
        z-index: 8;
        width: 200px;
        pointer-events: none;;
    }

    .menu-links.toggle-visible .parent-list{
        bottom: calc(100% + 10px);
        opacity: 1;
        visibility: visible;
        pointer-events: unset;
    }

    .menu-links .parent-list .item-list:before{
        content: attr(data-title);
        margin-bottom: 10px;
        display: block;
        text-align: initial;
        font-weight: 500;
        color: var(--color-main);
    }

    .menu-links .parent-list .item-list{
        margin-right: 0;
        display: block;
        margin-bottom: 20px;
    }

    .menu-links .parent-list .item-list:last-child{
        margin-bottom: 0;
    }

    .parent-list .item-list>a{
        display: none;
    }

    .parent-list .child-list{
        position: static;
        opacity: 1;
        visibility: visible;
        border: 0;
        padding: 0;
    }

    .parent-list .child-list li{
        display: inline-block;
        margin-right: 7px;
        margin-bottom: 7px;
    }

    .parent-list .child-list li a{
        border-width: 1px;
        border-style: solid;
        border-color: var(--color-border);
        border-radius: 30px;
        padding: 0 15px;
        height: 30px;
        line-height: 28px;
        display: block;
    }

    .parent-list .child-list li.active a, 
    .parent-list .child-list a:hover{
        background-color: var(--background-button);
        color: var(--color-button);
        border-color: var(--border-button);
    }
}

@media(max-width: 575px){
    .header .header-control .display-mobile{
        display: none;
    }

    .header-nav-mobile{
        display: flex;
        flex-wrap: wrap;
        margin-right: 0;
    }

    .logo-mobile{
        flex: 1;
        justify-content: center;
    }

    .logo-mobile a{
        text-align: center;
    }

    .cart-block .total-price span{
        display: none;
    }

    .header-tool{
        flex: initial;
        padding-left: 0;
    }
}

@media(max-width: 319px){
    .logo-mobile img{
        max-width: 80% !important;
    }

    .logo-mobile a{
        display: inline-block;
    }
}