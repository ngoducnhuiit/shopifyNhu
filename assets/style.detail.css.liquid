.template-product header{
    position: relative;
    z-index: 2;
}

.template-product main{
    position: relative;
    z-index: 1;
}

.template-product .breadcrumbs {
    padding: 21px 0;
}

.template-product .breadcrumbs .outline-breadcrum .wrap-breadcrum ul li:last-child span{
	color: var(--color-hover);
    font-size: 14px;
    text-transform: capitalize;
    font-weight: 500;
}

.details-info .spr-icon-star-half-alt:before,
.details-info .spr-icon-star:before{
	font-size: 13px;
	color: var(--color-star-product);
}

.details-info .spr-icon-star-empty:before{
	color: #ccc;
	font-size: 13px;
	content: '\e800';
}

.details-info .spr-icon{
	margin-right: 5px;
}

.details-info .spr-icon:last-child{
	margin-right: 0;
}

.details-info .spr-badge .spr-starrating{
	margin-right: 5px;
}

.details-info .spr-badge .spr-badge-caption{
	text-transform: capitalize;
	color: var(--color-text-body);
	font-size: 12px;
    line-height: 27px;
}

.details-info .spr-badge .spr-badge-caption:before{
	content:"(";
}

.details-info .spr-badge .spr-badge-caption:after{
	content:")";
}

.details-info .spr-badge{
	margin-right: 5px;
}

.details-info .spr-badge >*{
	display: inline-block;
	vertical-align: middle;
}

.details-info .star-rating>*{
	display: inline-block;
	vertical-align: middle;
}

.details-info .star-rating .write-review{
	font-size: 12px;
	line-height: 27px;
	color: var(--color-text-body);
}

.details-info .star-rating .write-review:hover{
	color: var(--color-hover);
}

.details-info .price {
	margin-top: 15px;
}

.details-info .price .money{
    font-weight: 600;
	font-size: 24px;
    line-height: 1;
	color: var(--color-hover);
}

.details-info .price--sale{
	margin-top: 0;
}

.details-info .price--sale .compare .money{
	font-size: 16px;
	color: #bcbcbc;
	font-weight: 400;
	text-decoration: line-through;
}

.details-info .price--sale .current .money{
	margin-right: 5px;
}

.details-info .free-shipping{
	text-align: initial;
}

.details-info .product_description{
	border-width: 1px 0 0;
	border-style: solid;
	border-color: var(--color-border);
}

.details-info .cms-product-meta{
	border-width: 1px 0 0;
	border-style: solid;
	border-color: var(--color-border);
	margin-top: 30px;
	padding-top: 30px;
	display: flex;
	flex-wrap: wrap;
}

.details-info .group-quantity-button{
	display: flex;
	flex-wrap: wrap;
	border-width: 1px;
	border-style: solid;
	border-color: var(--color-border);
	border-radius: 30px;
	width: 90px;
    height: 44px;
	line-height: 42px;
	margin-right: 20px;
}

.details-info .group-quantity-button >*{
	width: calc(100% /3);
	text-align: center;
}

.details-info .group-quantity-button input{
	border-width: 0;
}

.details-info .group-button{
	flex: 1;
}

.details-info .group-button .add-to-cart{
	padding: 0 25px;
    line-height: 38px;
    height: 44px;
    display: flex;
    align-items: center;
    justify-content: center;
    white-space: nowrap;
    font-weight: 500;
    font-size: 14px;
    border-radius: 30px;
	max-width: 158px;
	color: var(--color-button);
	background-color: var(--background-button);
}

.details-info .group-button .add-to-cart:hover{
	color: var(--color-button-hover);
	background-color: var(--background-button-hover);
}

.details-info .group-button .add-to-cart .icon{
	margin-right: 10px;
	font-size: 18px;
}

.details-info .group-button .add-to-cart .icon-loading{
	display: none;
    font-size: 15px;
    -webkit-animation: rotate-center .75s linear infinite both;
    animation: rotate-center .75s linear infinite both;
    margin: 0!important;
}

.details-info .group-button .ajax_loading .icon.icon-loading{
	display: inline-block;
	vertical-align: middle;
}

.details-info .group-button .add-to-cart.ajax_loading .text,
.details-info .group-button .add-to-cart.ajax_loading .icon:not(.icon-loading){
	display:none;
}

.product-collection__options {
	padding-top: 30px;
	margin-top: 30px;
	border-width: 1px 0 0;
	border-style: solid;
	border-color: var(--color-border);
}

.product-collection__options .cms-option-item label{
	font-weight: 600;
	color: var(--color-main);
}

.product-collection__options .cms-option-item .label-selected{
font-weight: 400;
}

.product-collection__options .cms-option-item .product-options__section{
margin-top: 10px;
}

.product-collection__options .cms-option-item .product-options__section .product-options__value{
margin-bottom: 10px;
}

.product-collection__options .cms-option-item{
margin-bottom: 20px;
}

.product-collection__options .cms-option-item:last-child{
margin-bottom: 0;
}

.details-info .group-button-buyitnow{
	margin-top: 30px;
	padding-top: 30px;
	border-width: 1px 0 0;
	border-style: solid;
	border-color: var(--color-border);
}

.details-info .prpage-term-conditions-checkbox label{
	margin: 0;
	font-size: 14px;
	font-weight: 400;
	cursor: pointer;
}

.details-info .prpage-term-conditions-checkbox a{
	text-decoration: underline;
	font-style: italic;
}

.details-info .prpage-term-conditions-checkbox a:hover{
	text-decoration: none;
}

.details-info .group-button-buyitnow .shopify-payment-button{
	margin-top: 20px;
}

.details-info .group-button-buyitnow .shopify-payment-button .shopify-payment-button__button{
	background-color: var(--background-button);
	color: var(--color-button);
	border-radius: 30px;
}

.details-info .group-button-buyitnow .shopify-payment-button .shopify-payment-button__button:hover{
	background-color: var(--background-button-hover);
	color: var(--color-button-hover);
}

.details-info .shopify-payment-button__more-options{
	border-radius: 30px;
	display: block;
	font-size: 14px;
	font-weight: 500;
	padding: 0 20px;
	text-align: center;
	width: 100%;
	transition: background 0.2s ease-in-out;
	height: 42px;
	line-height: 42px;
	background-color: var(--background-button);
	color: var(--color-button);
}

.details-info .shopify-payment-button__more-options:hover{
	background-color: var(--background-button-hover);
	color: var(--color-button-hover);
	text-decoration: none;
}

.details-info .shopify-payment-button__more-options.shopify-payment-button__button--hidden{
	display: none;
}

.details-info .compare-wishlist{
	margin-top: 20px;
}

.details-info .compare-wishlist .action{
	padding: 0;
	margin-right: 30px;
	font-size: 13px;
	font-weight: 600;
	color: var(--color-main);
}

.details-info .compare-wishlist [data-button-status="added"]{
	cursor: not-allowed;
}

.details-info .compare-wishlist .action:hover{
	color: var(--color-hover);
}

.details-info .compare-wishlist .action .icon{
	margin-right: 10px;
	vertical-align: middle;
	display: inline-block;
}

.details-info .compare-wishlist .action .text{
	vertical-align: middle;
	display: inline-block;
}

.details-info .compare-wishlist .action .icon.added,
.details-info .compare-wishlist .action .text.added{
	display: none;
}

.details-info .compare-wishlist [data-button-status=added] .icon,
.details-info .compare-wishlist [data-button-status=added] .text{
	display: none;
}

.details-info .compare-wishlist [data-button-status=added] .icon.added,
.details-info .compare-wishlist [data-button-status=added] .text.added{
	display: inline-block;
	vertical-align: middle;
}

.name_category p{
	margin: 0;
}

.name_category .label{
	font-weight: 400;
	margin-right: 5px;
}

.name_category .collection_item{
	font-weight: 600;
	color: var(--color-hover);
}

.name_category .collection_item:hover{
	text-decoration: underline;
}

.product-collection__info p{
	margin: 0;
}

.product-collection__info .label{
	font-weight: 400;
	margin-right: 5px;
}
.product-collection__info .value{
	font-weight: 600;
}

.product-collection__info a{
	color: var(--color-hover);
	display: inline-block;
}

.product-collection__info a:hover{
	text-decoration: underline;
}

.name_category .label+.space,
.tags_product .label+.space{
	display: none;
}

.details-info .custom_html{
	margin-top: 20px;
}

.details-info .custom_html .title{
	font-size: 12px;
	font-weight: 600;
	margin-bottom: 15px;
	display: block;
	margin-top: 0;
}

.details-info .payment_type_img img{
	display: inline-block;
	width: auto;
	padding: 0;
	margin-right: 5px;
}

.details-info .product_page_social label{
	display: none;
}

.details-info .product_page_social ul li{
	display: inline-block;
	margin-right: 5px;
}

.details-info .product_page_social ul li a{
	width: 30px;
	height: 30px;
	line-height: 30px;
	text-align: center;
	border-radius: 30px;
	display: inline-block;
}

.details-info .product_page_social ul li a .title{
	display: none;
}

.details-info .product_page_social ul li.facebook a{
	background-color: #3b5998;
	color: white;
}

.details-info .product_page_social ul li.facebook a:hover{
	background-color: #2d4373;
}

.details-info .product_page_social ul li.twitter a{
	background-color: #00aced;
	color: white;
}

.details-info .product_page_social ul li.twitter a:hover{
	background-color: #0087ba;
}

.details-info .product_page_social ul li.email a{
	background-color: #dd4b39;
	color: white;
}

.details-info .product_page_social ul li.email a:hover{
	background-color: #c23321;
}

.details-info .product_page_social ul li.pinterest a{
	background-color: #cb2027;
	color: white;
}

.details-info .product_page_social ul li.pinterest a:hover{
	background-color: #9f191f;
}

.details-info .product_page_social ul li.tumblr a{
	background-color: #35465C;
	color: white;
}

.details-info .product_page_social ul li.tumblr a:hover{
	background-color: #2d4058;
}

.details-info .product_page_social ul li.telegram a{
	background-color: #0099E5;
	color: white;
}

.details-info .product_page_social ul li.telegram a:hover{
	background-color: #005983;
}

.details-info .product_page_social ul li.whatsapp a{
	background-color: #4FCE5D;
	color: white;
}

.details-info .product_page_social ul li.whatsapp a:hover{
	background-color: #37b645;
}

.js-sticky-sidebar{
	position: relative;
}

.js-sticky-sidebar .price-sale-details{
	position: absolute;
	top: 20px;
	left: 20px;
	z-index: 3;
	font-size: 14px;
	font-weight: 500;
	line-height: 22px;
	border-radius: 30px;
	padding: 0 8px;
	height: 22px;
	color: white;
	background-color: var(--color-label-sale);
}

.details-thumb .wrap-main-slider{
	border-width: 1px;
    border-style: solid;
    border-color: var(--color-border);
    border-radius: 12px;
	position: relative;
	box-shadow: 0 0 2px rgba(0, 0, 0, 0.1);
}

.details-thumb div[data-slide-main]{
	opacity: 0;
    filter: alpha(opacity=0);
	-webkit-transition: all .3s ease-in-out 0s;
	-o-transition: all .3s ease-in-out 0s;
	transition: all .3s ease-in-out 0s;
}

.details-thumb div[data-slide-main].slick-slider{
	opacity: 1;
    filter: alpha(opacity=100);
}

.details-thumb .wrap-main-slider img{
	border-radius: 12px;
}

.details-thumb .zoomContainerAppendTo .zoomContainer{
	top: 0 !important;
	left: 0 !important;
}

.details-thumb .wrap-main-slider .zoomLens{
	-webkit-border-radius: 50%;
    -moz-border-radius: 50%;
    border-radius: 50%;
    -webkit-box-shadow: 0 0 25px rgb(0 0 0 / 80%) inset;
    -moz-box-shadow: 0 0 25px rgba(0, 0, 0, 0.8) inset;
    box-shadow: 0 0 25px rgb(0 0 0 / 80%) inset;
    border-width: 3px !important;
    z-index: 3 !important;
    opacity: 1 !important;
	border-color: white !important;
	background-color: transparent !important;
}

.details-thumb .zoomWindowContainer .zoomWindow{
	border-radius: 12px;
	border-color: var(--color-border)!important;
	margin-left: 10px;
}

.details-thumb .main_appendArrows{
	display: none;
}

.details-thumb .open_gallery{
	font-size: 0;
	position: absolute;
	top: 20px;
	right: 20px;
    display: block;
    height: 40px;
    width: 40px;
    border-radius: 12px;
    z-index: 80;
	color: var(--color-button);
    background-color: var(--background-button);
    text-align: center;
    text-indent: -9999px;
    overflow: hidden;
	cursor: pointer;
}

.details-thumb .open_gallery:hover{
	color: var(--color-button-hover);
    background-color: var(--background-button-hover);
}

.details-thumb .open_gallery .icon-fullscreen:before{
	font-family: 'organey-icon';
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    display: inline-block;
    font-style: normal;
    font-variant: normal;
    font-weight: 400;
    content: "\e06b";
    display: block;
    line-height: 40px;
    text-indent: 0;
    font-size: 18px;
}

.details-thumb .slider-nav .thumb_img .pr_lazy_img{
	border-width: 1px;
	border-style: solid;
	border-color: var(--color-border);
	border-radius: 8px;
}

.details-thumb .slider-nav .slick-center.slick-current .pr_lazy_img{
	border-color: var(--color-hover);
	box-shadow: 0 0 2px rgba(0, 0, 0, .3);
}

.details-thumb .slider-nav{
	margin-top: 10px;
}

.details-thumb .slider-nav .slick-slide{
	opacity: 1; 
}

.tab_product_page .cms-tab .nav>li>a:before{
	width: 0;
	height: 0;
	overflow: hidden;
}

.tab_product_page .cms-tab{
	border-radius: 8px;
	display: block;
	border-width: 1px;
	border-style:solid;
	border-color: var(--color-border);
	clear: both;
	overflow: hidden;
}

.tab_product_page .cms-tab .nav{
	padding: 0;
	margin: -1px -1px 0;
}

.tab_product_page .cms-tab .nav li{
	flex: 1;
	background-color: #f6f7f7;
	border-width: 0 0 0 1px;
	border-style: solid;
	border-color: #fff;
	text-align: center;
	padding: 0;
}

.tab_product_page .cms-tab .nav li:first-child{
	border-radius: 8px 0 0 0;
}

.tab_product_page .cms-tab .nav li:last-child{
	border-radius: 0 8px 0 0;
}

.tab_product_page .cms-tab .nav li.active{
	background-color: var(--background-button);
}

.tab_product_page .cms-tab .nav li a{
	font-size: 14px;
	display: block;
	position: relative;
	font-weight: 600;
	letter-spacing: 1px;
	text-transform: uppercase;
	color: var(--color-main);
	transition: all 0s ease;
	line-height: initial;
	padding: 14px 7px;
}

.tab_product_page .cms-tab .nav li.active a{
	color: var(--color-button);
}

.tab_product_page .cms-tab .tab-content .title-click-show{
	display: none;
}

.tab_product_page .cms-tab .tab-content{
	padding: 30px;
}

.tab_product_page .cms-tab .tab-content .description{
	font-size: 15px;
}

.tab_product_page .cms-tab .tab-content .sp-tab-content table{
	border-width: 0;
	margin: 0;
}

.tab_product_page .cms-tab .tab-content .sp-tab-content table tr th,
.tab_product_page .cms-tab .tab-content .sp-tab-content table tr td{
	padding: 14px 20px;
	border-width: 1px;
	border-color: var(--color-border);
	vertical-align: top;
	word-break: break-all;
	vertical-align: middle;
}

.tab_product_page .cms-tab .tab-content .sp-tab-content table tr th{
	color: var(--color-main);
	width: 35%;
	border-right-width: 0;
}

.tab_product_page .spr-header-title{
	display: none;
}

#shopify-product-reviews{
	margin: 0 !important;
}

.tab_product_page .spr-summary{
	text-align: center;
	display: flex;
	flex-wrap: wrap;
	position: relative;
}

.tab_product_page .spr-summary .spr-starrating{
	display: block;
	width: 100%;
	max-width: calc(100% - 410px);
}

.tab_product_page .spr-summary .spr-icon{
	color: var(--color-star-product);
	font-size: 25px;
	margin-right: 5px;
}

.tab_product_page .spr-summary .spr-icon:last-child{
	margin-right: 0;
}

.tab_product_page .spr-summary .spr-summary-caption{
	display: block;
	font-size: 12px;
	color: #9f9f9f;
	width: 100%;
	max-width: calc(100% - 410px);
}

.tab_product_page .spr-summary .spr-summary-caption:before{
	content:"(";
}

.tab_product_page .spr-summary .spr-summary-caption:after{
	content:")";
}

.tab_product_page .spr-summary .spr-summary-actions{
	width: 100%;
	max-width: 410px;
	text-align: center;
	position: absolute;
	top: 0;
	right: 0;
	bottom: 0;
	display: flex;
	flex-wrap: wrap;
	flex-direction: column;
	justify-content: center;
	border-width: 0 0 0 1px;
	border-style: solid;
	border-color: var(--color-border);
}

.tab_product_page .spr-summary .spr-summary-actions .spr-summary-actions-newreview{
	float: none;
	color: var(--border-button);
	border-color: var(--border-button);
	text-transform: uppercase;
	padding: 10px 20px;
	border-width: 1px;
	border-style:solid;
	font-size: 14px;
	border-radius: 30px;
	font-weight: 600;
	display: inline-block;
	max-width: 200px;
	margin: 0 auto;
}

.tab_product_page .spr-summary .spr-summary-actions .spr-summary-actions-newreview[style="display: none;"]{
	display: inline-block !important;
	cursor: not-allowed;
	opacity: .7
}

.tab_product_page .spr-summary .spr-summary-actions .spr-summary-actions-newreview:hover{
	color: var(--color-button);
	background-color: var(--background-button);
	border-color: var(--border-button);
}

.tab_product_page .spr-review{
	border-width: 0;
	padding-top: 0;
	margin-top: 0;
	margin-bottom: 37px;
	position: relative;
	padding-left: 70px;
}

.tab_product_page .spr-review .spr-icon{
	color: var(--color-star-product);
	font-size: 12px;
	margin-right: 3px;
}

.tab_product_page .spr-review .spr-icon:last-child{
	margin-right: 0;
}

.tab_product_page .spr-review .spr-review-header-title{
	display: none;
}

.tab_product_page .spr-review .spr-review-header-byline{
	display: block;
	font-size: 0;
}

.tab_product_page .spr-review:before{
	content:"";
	width: 50px;
	height: 50px;
	border-radius: 50%;
	overflow: hidden;
	background-image: url({{'avatar-mask.png' | asset_url }});
	background-size: cover;
	background-repeat: no-repeat;
	display: inline-block;
	position: absolute;
	top: 0;
	left: 0;
}

.tab_product_page .spr-review .spr-review-footer .spr-review-reportreview{
	float: none;
	display: inline-block;
	font-size: 12px;
	padding: 5px 20px;
	border-width: 1px;
	border-style: solid;
	border-color: var(--border-button);
	color: var(--border-button);
	border-radius: 30px;
}

.tab_product_page .spr-review .spr-review-footer .spr-review-reportreview:hover{
	color: var(--color-button);
	background-color: var(--background-button);
	border-color: var(--border-button);
}

.tab_product_page .spr-form{
    border-width: 0;
    margin-top: 32px;
    padding: 40px;
    background-color: #f5f5f5;
    border-radius: 12px;
}

.tab_product_page .spr-form .spr-form-title{
    margin-bottom: 20px;
}

.tab_product_page .spr-form fieldset{
    margin-bottom: 20px;
}

.tab_product_page .spr-form fieldset:last-child{
    margin-bottom: 0;
}

.tab_product_page .spr-form .spr-form-label{
	font-size: 12px;
	margin-bottom: 2px;
	line-height: 2;
	font-weight: 600;
}

.tab_product_page .spr-form input{
    padding: 10px 14px;
    background-color: white;
    border-color: var(--color-border);
    color: var(--color-main);
    border-width: 1px;
    border-style: solid;
    -webkit-appearance: none;
    box-sizing: border-box;
    font-weight: 400;
    border-radius: 6px;
    outline: 0;
    transition: all ease-in-out .3s;
}

.tab_product_page .spr-form .spr-form-input::placeholder{
    color: white;
}

.tab_product_page .spr-form .spr-form-input:focus{
    border-color: var(--color-hover);
}

.tab_product_page .spr-form textarea{
    padding: 10px 14px;
    background-color: white;
    border-color: var(--color-border);
    color: var(--color-main);
    border-width: 1px;
    border-style: solid;
    -webkit-appearance: none;
    box-sizing: border-box;
    font-weight: 400;
    border-radius: 6px;
    outline: 0;
    transition: all ease-in-out .3s;
}

.tab_product_page .spr-form .spr-form-review-body{
	margin-bottom: 0;
}

.tab_product_page .spr-form .spr-form-actions .spr-button{
    float: none;
    border-radius: 30px;
    padding: 10px 25px;
    color: var(--color-button);
    background-color: var(--background-button);
    border-color: var(--border-button);
}

.tab_product_page .spr-form .spr-form-actions .spr-button:hover{
    color: var(--color-button-hover);
    background-color: var(--background-button-hover);
    border-color: var(--border-button-hover);
}

.tab_product_page .spr-icon-star-half-alt:before,
.tab_product_page .spr-icon-star:before{
	color: var(--color-star-product);
}

.tab_product_page .spr-icon-star-empty:before{
    content: '\e800';
	color: #ccc;
}

.tab-panel table,
.tab-panel table:not([class]) {
	border-width: 1px;
	border-style: solid;
	border-color: var(--color-border);
	font-size: 14px;
}

.tab-panel table tr td{
	border-width: 1px;
	border-style: solid;
	border-color: var(--color-border);
	padding: 10px;
	vertical-align: middle;
}

.laber_collection_related .wrap_title .section-title{
	width: 100%;
	font-size: 30px;
	font-weight: 600;
	color: var(--color-main);
	display: flex;
	flex-wrap: wrap;
	justify-content: center;
	align-items: center;
	line-height: initial;
}

.laber_collection_related .wrap_title .section-title:before{
	content: "";
	border-bottom: 1px solid;
	border-color: var(--color-border);
	flex: 1;
	margin-right: 10px;
}

.laber_collection_related .wrap_title .section-title:after{
	content: "";
	border-bottom: 1px solid;
	border-color: var(--color-border);
	flex: 1;
	margin-left: 10px;
}

.laber_collection_related .wrap_title .section-subtitle{
	text-align: initial;
}

.laber_collection_related .wrap_title .content{
	padding-top: 0;
	margin-top: 5px;
	font-size: 14px;
	text-align: initial;
}

.laber_collection_viewed .wrap_title .section-subtitle{
    margin-bottom: 0;
	text-align: initial;
}

.laber_collection_viewed .wrap_title .section-title{
	width: 100%;
	font-size: 30px;
	font-weight: 600;
	color: var(--color-main);
	display: flex;
	flex-wrap: wrap;
	justify-content: center;
	align-items: center;
	line-height: initial;
}

.laber_collection_viewed .wrap_title .section-title:before{
    content: "";
    border-bottom: 1px solid;
    border-color: var(--color-border);
    flex: 1;
    margin-right: 10px;
}

.laber_collection_viewed .wrap_title .section-title:after{
	content: "";
	border-bottom: 1px solid;
	border-color: var(--color-border);
	flex: 1;
	margin-left: 10px;
}

.laber_collection_viewed .wrap_title .content{
	padding-top: 0;
	margin-top: 5px;
	font-size: 14px;
	text-align: initial;
}

/* sticky cart */
#shopify-section-sticky_add_to_cart{
	position: fixed;
	right: 0;
	bottom: 0;
	left: 0;
	z-index: 3;
	transform: translateY(100%);
	transition: all .5s ease;
}

.show-sticky #shopify-section-sticky_add_to_cart{
	transform: translateY(0);
}

.scroll-bottom-page #shopify-section-sticky_add_to_cart{
	transform: translateY(100%);
}

.sticky_atc_wrapper{
	padding: 7px 0;
}

.sticky_atc_thumb{
	max-width: 65px;
	border-width: 1px;
	border-style: solid;
	border-color: var(--color-border);
	border-radius: 5px;
	overflow: hidden;
	display: flex;
	flex-wrap: wrap;
}

.sticky_atc_content{
	display: flex;
	flex-wrap: wrap;
	align-items: center;
	flex: 1;
}

.sticky_atc_content .sticky_atc_info{
	padding-left: 16px;
}

.sticky_atc_price .money{
	font-weight: 600;
    font-size: 14px;
    line-height: 1;
}

#shopify-section-sticky_add_to_cart .spr-icon-star-half-alt:before,
#shopify-section-sticky_add_to_cart .spr-icon-star:before{
	color: var(--color-star-product);
    font-size: 13px;
}

#shopify-section-sticky_add_to_cart .spr-icon-star-empty:before{
    content: '\e800';
    color: #ccc;
    font-size: 13px
}

#shopify-section-sticky_add_to_cart .spr-badge-caption{
	display: none;
}

.sticky_atc_content .sticky_atc_info .name_product{
	margin: 0;
}

.sticky_atc_btn {
	display: flex;
	flex-wrap: wrap;
	align-items: center;
}

.sticky_atc_btn .quantity{
    border-width: 1px;
    border-style: solid;
    border-color: #d9e8db;
    border-radius: 30px;
    display: flex;
    flex-wrap: wrap;
    height: 44px;
    width: 88px;
    padding: 0 5px;
	margin-right: 8px;
	background-color: white;
}

.sticky_atc_btn .quantity input{
	border-width: 0;
	width: 32px;
	text-align: center;
	font-size: 14px;
	font-weight: 600;
	line-height: 22px;
	background-color: transparent;
}

.sticky_atc_btn .quantity .js_plus,
.sticky_atc_btn .quantity .js_minus{
	flex: 1;
	line-height: 22px;
	text-align: center;
	display: flex;
	flex-wrap: wrap;
	flex-direction: column;
	justify-content: center;
}

.sticky_atc_btn .sticky_atc_js{
	font-size: 14px;
    font-weight: 400;
    padding: 0 13px;
    display: block;
    border-width: 1px;
    border-style: solid;
    border-radius: 30px;
    text-align: center;
    height: 44px;
    line-height: 42px;
}

.sticky_atc_btn .sticky_atc_js .icon {
	margin-right: 10px;
    font-size: 18px;
	display: inline-block;
	vertical-align: middle;
}

.sticky_atc_btn .sticky_atc_js span{
	display: inline-block;
	vertical-align: middle;
}

.sticky_variant_content{
	margin-right: 20px;
}
/* sticky cart */

/* Resposive */
@media(max-width: 1199px){
	.tab_product_page .spr-summary .spr-starrating,
	.tab_product_page .spr-summary .spr-summary-caption{
		max-width: 60%;
	}

	.tab_product_page .spr-summary .spr-summary-actions{
		max-width: 40%;
	}
}

@media(max-width: 991px){
	#js-stickySidebar{
		position: static !important;
		z-index: 101;
	}

	#js-stickySidebar .StickySidebar{
		position: static !important;
	}

	.details-info{
		margin-top: 30px;
	}

	.zoomContainerAppendTo{
		display: none;
	}

	.details-thumb .wrap-main-slider [data-slide-main]{
		pointer-events: none;
	}

	.tab_product_page .cms-tab .nav{
		align-items: initial;
	}

	.tab_product_page .cms-tab .nav li{
		display: flex;
		flex-wrap: wrap;
		align-items: center;
		text-align: center;
		justify-content: center;
	}

	#shopify-section-sticky_add_to_cart{
		display: none;
	}
}

@media(max-width: 767px){
	.tab_product_page .cms-tab .tab-content{
		padding: 0;
	}

	.tab_product_page .cms-tab .nav{
		display: none;
	}

	.tab_product_page .cms-tab .tab-content .title-click-show{
		display: flex;
		flex-wrap: wrap;
		justify-content: space-between;
		padding: 10px 20px;
		text-transform: uppercase;
		background-color: var(--background-button);
		color: var(--color-button);
	}

	.tab_product_page .cms-tab .tab-content .title-click-show:after{
		content: "\e9b1";
		font-family: 'icomoon' !important
	}

	.tab_product_page .cms-tab .tab-content .parent-click-show.active .title-click-show:after{
		content: "\e996";
		font-family: 'icomoon' !important
	}

	.tab_product_page .cms-tab .tab-content .content-click-show{
		padding: 20px;
		display: none;
	}

	.tab_product_page .cms-tab .tab-content .parent-click-show {
		border-width: 0 0 1px;
		border-style: solid;
		border-color: var(--color-border);
	}

	.tab_product_page .cms-tab .tab-content .tab-panel:last-child .parent-click-show {
		border-width: 0;
	}

	.tab_product_page .cms-tab .tab-content .tab-panel{
		position: static;
		opacity: 1;
		visibility: visible;
	}
}

@media(max-width: 575px){
	.tab_product_page .cms-tab .nav li{
		min-width: 200px;
	}

	.tab_product_page .spr-summary .spr-starrating, 
	.tab_product_page .spr-summary .spr-summary-caption{
		max-width: 100%;
	}

	.tab_product_page .spr-summary .spr-summary-actions{
		max-width: 100%;
		border-width: 0;
		margin-top: 15px;
		position: static;
	}

	.tab_product_page .spr-form{
		padding: 20px;
	}

	.tab_product_page .cms-tab .tab-content .content-click-show{
		padding: 15px;
	}
}