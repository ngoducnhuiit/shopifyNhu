}

.product-collection__reviews .spr-icon{
  margin-right: 5px;
}

.product-collection__reviews .spr-icon:last-child{
  margin-right: 0;
}

.product-collection__reviews{
  position: relative;
  overflow: hidden;
}

.product-collection__reviews:before{
  content: "";
  position: absolute;
  z-index: 996;
  animation-duration: 1s;
  animation-fill-mode: forwards;
  animation-iteration-count: infinite;
  animation-name: placeHolderShimmer;
  animation-timing-function: linear;
  background: #f6f7f8;
  background: linear-gradient(to right,#eeeeee 8%,#dddddd 18%,#eeeeee 33%);
  background-size: 800px 104px;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
  visibility: hidden;
}

.ajax_loading .product-collection__reviews:before,
.page_loading .product-collection__reviews:before,
.openloadding .product-collection__reviews:before{
  opacity: 1;
  visibility: visible;
}

