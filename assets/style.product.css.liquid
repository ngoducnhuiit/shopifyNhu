.product-item{
  padding-top: 20px;
  padding-bottom: 20px;
}

.product-item .inner-product-item{
  position: relative;
}

.product-item .inner-product-item:before{
  content: "";
  position: absolute;
  top: 50%;
  left: 50%;
  width: 100%;
  height: 100%;
  transform: translate(-50%,-50%);
  transition: all .3s ease-in-out;
  border-radius: 16px;
  box-shadow: 0 0 24px rgba(0, 0, 0, 0.1);
  opacity: 0;
  z-index: -1;
}

.product-item .inner-product-item:hover:before{
  opacity: 1;
  width: calc(100% + 40px);
  height: calc(100% + 40px);
}

/* product item image */
.product-item .product-item-image{
  position: relative;
  -ms-transition: opacity 1.5s cubic-bezier(.19,1,.22,1) 0ms;
  -o-transition: opacity 1.5s cubic-bezier(.19,1,.22,1) 0ms;
  -moz-transition: opacity 1.5s cubic-bezier(.19,1,.22,1) 0ms;
  -webkit-transition: opacity 1.5s cubic-bezier(.19,1,.22,1) 0ms;
  transition: opacity 1.5s cubic-bezier(.19,1,.22,1) 0ms;
  overflow: hidden;
}

.inner-product-item:hover .product-item-image .product-item-image-link{
  opacity: .3;
}

.product-item .product-item-image .pr_lazy_img.laber_img_2{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  z-index: 1;
  opacity: 0;
  visibility: hidden;
  transition: all .3s ease;
  background-color: white;
}

.product-item .product-item-image:hover .pr_lazy_img.laber_img_2{
  opacity: 1;
  visibility: visible;
}

.product-item .product-item-image:before{
  content: "";
  position: absolute;
  z-index: 996;
  animation-duration: 1s;
  animation-fill-mode: forwards;
  animation-iteration-count: infinite;
  animation-name: placeHolderShimmer;
  animation-timing-function: linear;
  background: #f6f7f8;
  background: linear-gradient(to right,#eeeeee 8%,#dddddd 18%,#eeeeee 33%);
  background-size: 800px 104px;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
  visibility: hidden;
}

.ajax_loading .product-item .product-item-image:before,
.page_loading .product-item .product-item-image:before,
.openloadding .product-item .product-item-image:before{
  opacity: 1;
  visibility: visible;
}

.product-item.product-item-sold-out .inner-product-item .product-item-image .product-item-image-link{
  opacity: .3
}
/* end product item image */

/* product item tool */
.product-item .product-item-tool .action{
  display: inline-block;
  width: 40px;
  height: 40px;
  line-height: 40px;
  border-radius: 50%;
  border-width: 1px;
  border-style: solid;
  color: var(--color-button);
  background-color: var(--background-button);
  border-color: var(--border-button);
  text-align: center;
  padding: 0;
  vertical-align: middle;
  margin-right: 8px;
}

.product-item .product-item-tool .action:last-child{
  margin-right: 0;
}

.product-item .product-item-tool .action:hover{
  color: var(--color-button-hover);
  background-color: var(--background-button-hover);
  border-color: var(--border-button-hover);
}

.product-item .product-item-tool .action .text{
  display: none;
} 

.product-item .product-item-tool .action .icon{
  font-size: 15px;
}

.product-item .product-item-tool .action .icon.added{
  display: none;
}

.product-item .product-item-tool .icon.icon-loading{
  display: none;
  -webkit-animation: rotate-center .75s linear infinite both;
  animation: rotate-center .75s linear infinite both;
  margin: 0 !important;
}

.product-item .product-item-tool .ajax_loading .icon{
  display: none;
}

.product-item .product-item-tool .ajax_loading .icon.icon-loading{
	display: inline-block;
	vertical-align: middle;
}


.product-item .product-item-tool .action[data-button-status="added"]{
  cursor: not-allowed;
}

.product-item .product-item-tool .action[data-button-status="added"] .icon{
  display: none;
}

.product-item .product-item-tool .action[data-button-status="added"] .icon.added{
  display: inline-block;
}

.product-item .product-item-tool{
  position: absolute;
  top: calc(50% + 50px);
  transform: translateY(-50%);
  left: 0;
  right: 0;
  z-index: 2;
  transition: all .3s ease;
  opacity: 0;
  visibility: hidden;
  text-align: center;
}

.product-item .inner-product-item:hover .product-item-tool{
  top: 50%;
  opacity: 1;
  visibility: visible;
}

.list-style .product-item .product-item-tool.list-style-none{
